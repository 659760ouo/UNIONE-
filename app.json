{
  "expo": {
    "name": "yrsdaily",
    "slug": "yrsdaily",
    // "scheme": "com.yrsdaily.unione",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yrsdaily.unione"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/unione.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yrsdaily.unione"

    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "android": {
            "googleServicesFile": "./app/google-services.json",
            
          }
        }
      ],
    ],
    "experiments": {
      "typedRoutes": true
    }
    
  }
}
