{
  "expo": {
    "name": "unione",
    "slug": "unione",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/unione.png",
    "scheme": "goaltracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "sdkVersion": "53.0.0",
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yrsdaily.unione"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cef43a3e-c20b-4067-a4e9-e6c1412678f2"
      }
    },
    "owner": "yrsdaily"
  }
}
